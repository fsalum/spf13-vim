" Fix Python issue on Mac
" https://github.com/spf13/spf13-vim/issues/176
let $PYTHONHOME='/System/Library/Frameworks/Python.framework/Versions/Current'

UnBundle 'Puppet-Syntax-Highlighting'
UnBundle 'godlygeek/csapprox'

set clipboard=unnamed "Fix for Mac to allow copy in visual mode to clipboard"
set virtualedit=      "Back to original mode"
